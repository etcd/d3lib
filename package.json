{
  "name": "etchart",
  "version": "1.0.11",
  "scripts": {
    "build:swc": "swc source/lib source/utilities -d build/ -C module.type=es6 -C sourceMaps=inline",
    "build:types": "yarn run tsc source/lib/*.{ts,tsx} --emitDeclarationOnly --outDir build/ --declaration --downlevelIteration --jsx react-jsx --esModuleInterop",
    "build": "yarn run build:swc; yarn run build:types",
    "dev": "vite",
    "typecheck": "tsc --noEmit --listFiles",
    "clean:build": "rimraf build",
    "clean:node_modules": "rimraf node_modules"
  },
  "type": "module",
  "exports": {
    ".": "./build/lib/ReactLineChart.js",
    "./build/lib/*": "./build/lib/*.js",
    "./build/utilities/*": "./build/utilities/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etcd/d3lib.git"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.42",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "autoprefixer": "^10.4.13",
    "esbuild": "^0.17.2",
    "eslint": "^8.32.0",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@visx/axis": "^3.0.0",
    "@visx/curve": "^3.0.0",
    "@visx/group": "^3.0.0",
    "@visx/legend": "^3.0.0",
    "@visx/scale": "^3.0.0",
    "@visx/shape": "^3.0.0",
    "@visx/tooltip": "^3.0.0",
    "d3": "^7.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
